<%- include("partials/header")  %> 


<h1>The Posts Page</h1>

<!-- Using EJS vscode extension: ejs, ejsout, ejsfor, ejseach, ejsif, etc... -->

<% for(let i=0; i<posts.length; i++){ %>
    <li>
        title = <%= posts[i].title %> - <strong><%= posts[i].author %></strong>
    </li>
<% } %>


<% posts.forEach(post => { %>
    <li>
        title = <%= post.title %> - <strong><%= post.author %></strong>
    </li>
<% }) %>

<%- include("partials/footer")  %> 
